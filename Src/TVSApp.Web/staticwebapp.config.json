{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/assets/*", "/img/*", "/css/*"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
